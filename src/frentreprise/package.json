{
  "name": "frentreprise",
  "version": "1.0.0",
  "description": "Get companies informations from french government",
  "main": "dist/frentreprise.js",
  "repository": "https://github.com/commit42/frentreprise",
  "author": "commit42",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@sentry/node": "^5.17.0",
    "config": "^3.3.1",
    "date-fns": "^2.12.0",
    "follow-redirects": "^1.10.0",
    "is-buffer": "^2.0.4",
    "lodash.get": "^4.4.2",
    "qs": "^6.9.2",
    "tunnel": "^0.0.6"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack --color --watch",
    "build": "NODE_ENV=production webpack --color",
    "test": "NODE_ENV=test jest",
    "test-watch": "npm run test -- --watchAll",
    "coverage": "rm -rf coverage/ && npm run test -- --coverage --coverageDirectory=coverage",
    "lint": "eslint --max-warnings=0 --ignore-path .gitignore src"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^23.6.0",
    "jest-babel": "^1.0.1",
    "jest-html-reporter": "^2.8.1",
    "prettier": "^2.0.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/config/",
      "<rootDir>/lib/axios"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/config/",
      "!<rootDir>/lib/axios"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
