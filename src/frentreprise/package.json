{
  "name": "frentreprise",
  "version": "1.0.0",
  "description": "Get companies informations from french government",
  "main": "dist/frentreprise.js",
  "repository": "https://github.com/commit42/frentreprise",
  "author": "commit42",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "follow-redirects": "^1.3.0",
    "is-buffer": "^1.1.5",
    "tunnel": "^0.0.5"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --color",
    "test": "NODE_ENV=test jest",
    "test-watch": "npm run test -- --watchAll",
    "coverage":
      "rm -rf coverage/ && npm run test -- --coverage --coverageDirectory=coverage"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^23.6.0",
    "jest-babel": "^1.0.1",
    "jest-html-reporter": "^2.4.2",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.9",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": ["<rootDir>/config/", "<rootDir>/lib/axios"],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/config/",
      "!<rootDir>/lib/axios"
    ],
    "coverageReporters": ["text", "lcov", "html"]
  }
}
