{
    "name": "n8n-nodes-starter",
    "version": "0.1.0",
    "description": "Example starter module for custom n8n nodes.",
    "license": "SEE LICENSE IN LICENSE.md",
    "homepage": "https://n8n.io",
    "author": {
        "name": "Jan Oberhauser",
        "email": "jan@n8n.io"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/n8n-io/n8n-nodes-starter.git"
    },
    "main": "index.js",
    "scripts": {
        "dev": "npm run watch",
        "build": "tsc && gulp",
        "tslint": "tslint -p tsconfig.json -c tslint.json",
        "watch": "tsc --watch",
        "test": "jest",
        "n8n": "export $(cat .env | xargs) && n8n",
        "n8n:export": "n8n export:workflow --backup --output ./export/workflows/ && n8n export:credentials --backup --output ./export/credentials --decrypted"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "credentials": [
            "dist/credentials/Minio.credentials.js"
        ],
        "nodes": [
            "dist/nodes/MinioDownload/MinioDownload.node.js",
            "dist/nodes/RupcoProcedureIngest/RupcoProcedureIngest.node.js",
            "dist/nodes/DeleteFile/DeleteFile.node.js"
        ]
    },
    "devDependencies": {
        "@types/csv-parse": "^1.2.2",
        "@types/csv-stringify": "^3.1.0",
        "@types/express": "^4.17.6",
        "@types/html-entities": "^1.3.4",
        "@types/jest": "^24.0.18",
        "@types/minio": "^7.0.8",
        "@types/multipipe": "^3.0.0",
        "@types/node": "^10.10.1",
        "@types/pg": "^8.6.1",
        "@types/pg-copy-streams": "^1.2.1",
        "@types/replacestream": "^4.0.1",
        "@types/request-promise-native": "^1.0.17",
        "gulp": "^4.0.0",
        "jest": "^24.9.0",
        "n8n": "^0.136.0",
        "n8n-workflow": "^0.66.0",
        "ts-jest": "^24.0.2",
        "tslint": "^5.17.0",
        "typescript": "~4.4.2"
    },
    "dependencies": {
        "csv-parse": "^4.16.0",
        "csv-stringify": "^5.6.2",
        "data-forge": "^1.8.17",
        "data-forge-fs": "^0.0.9",
        "date-fns": "^2.22.1",
        "date-fns-tz": "^1.1.6",
        "html-entities": "^2.3.2",
        "minio": "^7.0.18",
        "multipipe": "^4.0.0",
        "n8n-core": "^0.81.0",
        "pg": "^8.6.0",
        "pg-copy-streams": "^5.1.1",
        "replacestream": "^4.0.3"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "testURL": "http://localhost/",
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
        "testPathIgnorePatterns": [
            "/dist/",
            "/node_modules/"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ]
    }
}
