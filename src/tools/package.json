{
  "name": "fcetools",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "n8n": "export $(cat .env | xargs) && node_modules/.bin/n8n",
    "n8nd": "node_modules/.bin/n8n-node-dev",
    "dev": "npm run watch",
    "build": "tsc && gulp",
    "tslint": "tslint -p tsconfig.json -c tslint.json",
    "watch": "tsc --watch",
    "test": "jest"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "credentials": [
      "dist/nodes/SshCredentials.credentials.js"
    ],
    "nodes": [
      "dist/nodes/BackupPostgres.node.js",
      "dist/nodes/DownloadSireneFiles.node.js",
      "dist/nodes/DownloadSourceFile.node.js",
      "dist/nodes/ImportInteractionT.node.js",
      "dist/nodes/ImportSirene.node.js",
      "dist/nodes/ReindexAppsearch.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.7",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^14.0.25",
    "@types/request-promise-native": "^1.0.17",
    "gulp": "^4.0.2",
    "jest": "^26.1.0",
    "n8n-workflow": "^0.35.0",
    "ts-jest": "^26.1.3",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "n8n": "^0.74.0",
    "n8n-core": "^0.39.0",
    "n8n-node-dev": "^0.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/dist/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
